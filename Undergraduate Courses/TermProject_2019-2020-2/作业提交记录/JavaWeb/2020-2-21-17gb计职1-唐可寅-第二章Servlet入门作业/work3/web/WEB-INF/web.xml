<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>TestWebXmlServlet</servlet-name>
        <servlet-class>servlet.TestWebXmlServlet</servlet-class>
        <init-param>
            <param-name>data</param-name>
            <param-value>ServletConfig</param-value>
        </init-param>
        <init-param>
            <param-name>server</param-name>
            <param-value>1.1.1.1</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestWebXmlServlet</servlet-name>
        <url-pattern>/TestWebXmlServlet</url-pattern>
    </servlet-mapping>
</web-app>